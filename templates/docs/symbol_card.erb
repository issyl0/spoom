<div class="card">
  <div class="card-body">
    <%= foo %>
    <h5 class="card-title text-primary"><%= symbol_sig(symbol).html %></h5>
    <p class="card-text"></p>
    <% if comment_string %>
      <p><%= comment_string %></p>
    <% end %>
    <% if sig_string %>
      <pre class="bg-light"><code><%= sig_string %></code></pre>
    <% end %>
  </div>
</div>
